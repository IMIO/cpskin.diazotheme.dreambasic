<?xml version="1.0"?>
<object name="portal_css">

 <stylesheet title="cpskin.diazotheme.dreambasic styles"
    id="++theme++cpskin.diazotheme.dreambasic/css/styles.css"
    media="screen" rel="stylesheet" rendering="import"
    cacheable="True" compression="safe" cookable="True"
    enabled="1"
    expression="python:here.restrictedTraverse('@@currentTheme')() == 'cpskin.diazotheme.dreambasic'"
    applyPrefix="True"
    insert-before="ploneCustom.css"
    insert-after="++theme++cpskin.diazotheme.dream/css/styles.css"/>
  
  <stylesheet title="cpskin.diazotheme.dreambasic mobile style"
     id="++theme++cpskin.diazotheme.dreambasic/css/mobile-dreambasic.css"
     media="all"
     rel="stylesheet" rendering="import"
     cacheable="True" compression="safe" cookable="True"
     enabled="1" expression="request/HTTP_X_THEME_ENABLED | nothing"
     applyPrefix="True"
     insert-after="++theme++cpskin.diazotheme.dream/css/mobile-dream.css" />

</object>
